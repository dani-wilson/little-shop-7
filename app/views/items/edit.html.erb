<%= form_with(
  model: @item,
  url: merchant_item_path(@merchant, @item),
  local: true,
  data: { turbo: false }
  )do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :unit_price %>
  <%= f.text_field :unit_price %>
  <%= f.submit "Submit" %>
<% end %>


